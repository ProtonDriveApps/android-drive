  <string name="photos_share_migration_needed_banner_description">Después de la migración, puede usar nuevas funciones como filtros y álbum de fotos</string>
  <string name="photos_tags_migration_title">Migración de etiquetas</string>
  <!--
        Variable %1$s is a quantity (formatted) of photos migrates
        Example:
           Finished, 1 file were migrated.
           Finished, 12 345 files were migrated.
    -->
  <plurals name="photos_tags_migration_finished">
    <item quantity="one">Finalizado, %1$s archivo migrado.</item>
    <item quantity="other">Finalizado, %1$s archivos migrados.</item>
  </plurals>
  <!--
        Variable %1$s is a quantity (formatted) of photos to migrate
        Variable %1$s is a quantity (formatted) of photos waiting for download
        Example:
           Ongoing, migrating 1 file, 0 waiting for download.
           Ongoing, migrating 12 345 files, 123 waiting for download.
    -->
  <plurals name="photos_tags_migration_ongoing">
    <item quantity="one">Migrando %1$s archivo y %2$s en espera de descarga</item>
    <item quantity="other">Migrando %1$s archivos y %2$s en espera de descarga</item>
