<resources>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
  <plurals name="trash_operation_successful_format">
    <item quantity="one">1 elemento enviado a la papelera</item>
    <item quantity="other">%1$d elementos enviados a la papelera</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
  <plurals name="trash_restore_operation_successful_format">
    <item quantity="one">1 elemento restaurado</item>
    <item quantity="other">%d elementos restaurados</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="one">1 elemento eliminado de forma permanente</item>
    <item quantity="other">%d elementos eliminados de forma permanente</item>
  </plurals>
  <string name="trash_empty_operation_successful">Papelera vaciada</string>
  <string name="trash_error_occurred_sending_to_trash">Ocurrió un error al intentar enviar uno o más de los archivos a la papelera</string>
  <string name="trash_error_occurred_restoring_from_trash">Ocurrió un error al intentar restaurar uno o más de los archivos</string>
  <string name="trash_error_occurred_deleting_from_trash">Ocurrió un error al intentar eliminar uno o más de los archivos</string>
  <string name="trash_error_occurred_emptying_trash">Ocurrió un error al intentar vaciar la papelera</string>
</resources>
