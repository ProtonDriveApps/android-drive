<resources>
  <string name="photos_title">Φωτογραφίες</string>
  <string name="photos_permissions_title">Κρυπτογράφηση και δημιουργία αντιγράφων ασφαλείας των φωτογραφιών και των βίντεό σας</string>
  <string name="photos_permissions_section_1_title">Προστατέψτε τις αναμνήσεις σας</string>
  <string name="photos_permissions_section_1_description">Οι φωτογραφίες σας είναι κρυπτογραφημένες από άκρο σε άκρο, διασφαλίζοντας απόλυτο απόρρητο.</string>
  <string name="photos_permissions_section_2_title">Αυτόματη δημιουργία αντιγράφων ασφαλείας</string>
  <string name="photos_permissions_section_2_description">Οι φωτογραφίες δημιουργούνται τακτικά αντίγραφα ασφαλείας μέσω WiFi στην αρχική τους ποιότητα.</string>
  <string name="photos_permissions_action">Ενεργοποίηση αντιγράφων ασφαλείας</string>
  <string name="photos_permission_rational_img_text">Άδεια πρόσβασης</string>
  <!--
        Variable %1$s is an application name
        Example:
            "Proton Drive needs full access to your photos"
    -->
  <string name="photos_permission_rational_title">Το %1$s χρειάζεται πλήρη πρόσβαση στις φωτογραφίες σας</string>
  <!--
        Variable %1$s is an application name
        Example:
            "...available across all your devices.
             Proton Drive takes your privacy seriously..."
    -->
  <string name="photos_permission_rational_description">Αυτό είναι απαραίτητο για να διασφαλιστεί ότι δημιουργούνται αντίγραφα ασφαλείας όλων των φωτογραφιών σας και είναι διαθέσιμες σε όλες τις συσκευές σας.\nTo %1$s λαμβάνει σοβαρά υπόψη το απόρρητό σας και θα έχει πρόσβαση στις φωτογραφίες σας μόνο για σκοπούς συγχρονισμού και δημιουργίας αντιγράφων ασφαλείας.</string>
  <string name="photos_permission_rational_confirm_action">Ρυθμίσεις</string>
  <string name="photos_permission_rational_dismiss_action">Όχι τώρα</string>
  <string name="photos_empty_loading_label_encrypted">Κρυπτογραφημένο από άκρο σε άκρο (διατερματικά)</string>
  <string name="photos_empty_loading_label_progress">Η δημιουργία αντιγράφων ασφαλείας βρίσκεται σε εξέλιξη.\nΜπορεί να χρειαστεί λίγος χρόνος</string>
  <string name="photos_separator_current_month">Αυτό το μήνα</string>
  <string name="photos_encrypted_footer">Κρυπτογραφημένο από άκρο σε άκρο (διατερματικά)</string>
  <!--
        Variable %1$s is a folder name, %2$s is a quantity of folders found
        Example:
            "Backing up items from 1 folder: Camera"
            "Backing up items from 3 folders: Camera, Raw, Screenshots"
    -->
  <plurals name="photos_message_folders_setup">
    <item quantity="one">Δημιουργία αντιγράφων ασφαλείας στοιχείων από %2$d φάκελο: %1$s</item>
    <item quantity="other">Δημιουργία αντιγράφων ασφαλείας στοιχείων από %2$d φακέλους: %1$s</item>
  </plurals>
  <!--
        Variable %1$s is a folder name
        Example:
            "No Camera folder found for backup"
    -->
  <string name="photos_error_no_folders">Δεν βρέθηκε φάκελος %1$s για δημιουργία αντιγράφων ασφαλείας</string>
