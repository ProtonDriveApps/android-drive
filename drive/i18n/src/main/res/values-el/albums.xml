            "1 item failed"
            "23 items failed"
    -->
  <plurals name="in_app_notification_add_to_stream_failed">
    <item quantity="one">%1$d στοιχείο απέτυχε</item>
    <item quantity="other">%1$d στοιχεία απέτυχαν</item>
  </plurals>
