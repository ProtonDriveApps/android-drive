<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Compartilhar %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">1 pessoa adicionada</item>
    <item quantity="other">%1$d pessoas adicionadas</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">uma pessoa não pode ser convidada</item>
    <item quantity="other">%1$d pessoas não pode ser convidada</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Compartilhando com uma pessoa</item>
    <item quantity="other">Compartilhando com %1$d pessoas</item>
  </plurals>
  <string name="common_permission_owner">Proprietário</string>
  <string name="common_permission_admin">Administrador</string>
  <string name="common_permission_editor">Editor</string>
  <string name="common_permission_viewer">Visualizador</string>
  <string name="common_permission_unknown">Desconhecido</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (Pendente)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (Aceitado)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (Desconhecido)</string>
  <string name="share_via_invitations_add_people_hint">Adicionar pessoas</string>
  <string name="share_via_invitations_copy_invite_link_action">Copiar link de convite</string>
  <string name="share_via_invitations_remove_access_action">Remover o acesso</string>
  <string name="share_via_invitations_stop_sharing_description">Excluir o link e remover o acesso para todos</string>
  <string name="share_via_invitations_resend_invite_action">Reenviar convite</string>
  <string name="share_via_invitations_resend_invite_success">O convite foi reenviado.</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d membro</item>
    <item quantity="other">%1$d membros</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">Adicionar uma mensagem</string>
  <string name="share_via_invitations_send_message_and_name_label">Incluir mensagem e nome do arquivo no e-mail de convite.</string>
</resources>
