<resources>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
  <plurals name="trash_operation_successful_format">
    <item quantity="one">1 item enviado para a lixeira</item>
    <item quantity="other">%1$d itens enviados para a lixeira</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
  <plurals name="trash_restore_operation_successful_format">
    <item quantity="one">1 item restaurado</item>
    <item quantity="other">%d itens restaurados</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="one">1 item excluído permanentemente</item>
    <item quantity="other">%d itens excluídos permanentemente</item>
  </plurals>
  <string name="trash_empty_operation_successful">Lixeira esvaziada</string>
  <string name="trash_error_occurred_sending_to_trash">Ocorreu um erro ao tentar enviar um ou mais de seus arquivos para a lixeira.</string>
  <string name="trash_error_occurred_restoring_from_trash">Ocorreu um erro durante a tentativa de restaurar seu(s) arquivo(s)</string>
  <string name="trash_error_occurred_deleting_from_trash">Ocorreu um erro a tentar eliminar um ou mais arquivos.</string>
  <string name="trash_error_occurred_emptying_trash">Ocorreu um erro ao tentar vaziar a lixeira.</string>
</resources>
