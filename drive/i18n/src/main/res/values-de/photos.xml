<resources>
  <string name="photos_title">Fotos</string>
  <string name="photos_permissions_title">Verschlüssle und sichere deine Fotos und Videos</string>
  <string name="photos_permissions_section_1_title">Schütze deine Erinnerungen</string>
  <string name="photos_permissions_section_1_description">Deine Fotos werden Ende-zu-Ende verschlüsselt, sodass der Schutz deiner Daten gewährleistet ist.</string>
  <string name="photos_permissions_section_2_title">Automatische Sicherungen</string>
  <string name="photos_permissions_section_2_description">Fotos werden regelmäßig über WLAN in Originalqualität gesichert.</string>
  <string name="photos_permissions_action">Sicherung einschalten</string>
  <string name="photos_permission_rational_img_text">Zugriff erlauben</string>
  <!--
        Variable %1$s is an application name
        Example:
            "Proton Drive needs full access to your photos"
    -->
  <string name="photos_permission_rational_title">%1$s benötigt vollen Zugriff auf deine Fotos</string>
  <!--
        Variable %1$s is an application name
        Example:
            "...available across all your devices.
             Proton Drive takes your privacy seriously..."
    -->
  <string name="photos_permission_rational_description">Dies ist notwendig, um sicherzustellen, dass alle deine Fotos gesichert werden und auf all deinen Geräten verfügbar sind. \n%1$s nimmt deine Privatsphäre ernst und greift nur zum Zweck der Synchronisierung und Sicherung auf deine Fotos zu.</string>
  <string name="photos_permission_rational_confirm_action">Einstellungen</string>
  <string name="photos_permission_rational_dismiss_action">Nicht jetzt</string>
  <string name="photos_empty_loading_label_encrypted">Ende-zu-Ende-verschlüsselt</string>
  <string name="photos_empty_loading_label_progress">Die Sicherung ist im Gange.\n Es kann eine Weile dauern.</string>
  <string name="photos_separator_current_month">Dieser Monat</string>
  <string name="photos_encrypted_footer">Ende-zu-Ende-verschlüsselt</string>
  <!--
        Variable %1$s is a folder name, %2$s is a quantity of folders found
        Example:
            "Backing up items from 1 folder: Camera"
            "Backing up items from 3 folders: Camera, Raw, Screenshots"
    -->
  <plurals name="photos_message_folders_setup">
    <item quantity="one">Sichern von Elementen aus dem %2$d-Ordner: %1$s</item>
    <item quantity="other">Sichern von Elementen aus den %2$d-Ordnern: %1$s</item>
  </plurals>
  <!--
