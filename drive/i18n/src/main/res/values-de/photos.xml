            "1 item saved"
            "12 345 item saved"
    -->
  <plurals name="photos_backup_state_completed_items_saved">
    <item quantity="one">%1$s Eintrag gespeichert</item>
    <item quantity="other">%1$s Einträge gespeichert</item>
  </plurals>
