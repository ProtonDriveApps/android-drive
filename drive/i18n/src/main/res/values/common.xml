    <!-- region Common file and folder name validation errors -->
    <string name="common_error_name_is_blank">Name must not be empty</string>

    <!--
        Variable %1$d is a number
        Example:
            "Name must not exceed 280 characters"
    -->
    <string name="common_error_name_too_long">Name must not exceed %1$d characters</string>
    <string name="common_error_name_with_forbidden_characters">Name cannot include invisible characters, \"/\" or \"\\\"</string>
