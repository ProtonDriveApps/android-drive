    <string name="common_error_crypto">Encryption or decryption failed</string>
    <!--
        Variable %1$s is the non-null message from Crypto exception
        Example:
            "Encryption or decryption failed: Key cannot be used while inactive."
    -->
    <string name="common_error_crypto_with_message">Encryption or decryption failed: %1$s</string>
    <string name="common_error_io">Input or output operation failed</string>
