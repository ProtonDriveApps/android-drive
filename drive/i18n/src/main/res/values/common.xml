    <!--
        Variable %1$s is a percentage
        Examples:
            "0% downloaded"
            "50% downloaded"
            "100% downloaded"
    -->
    <string name="common_percent_downloaded">%1$s downloaded</string>
    <string name="common_sign_out">Sign out</string>
    <!--
        Variable %1$d is a quantity of files or folders that have been selected
        Example:
            "1 selected"
            "13 selected"
    -->
    <plurals name="common_selected">
        <item quantity="other">%1$d selected</item>
    </plurals>

    <!-- region In-app notification -->
    <!--
        Variable %1$s is label that is copied to clipboard
        Example:
            "Link copied to clipboard"
    -->
    <string name="common_in_app_notification_copied_to_clipboard">%1$s copied to clipboard</string>
    <string name="common_in_app_notification_download_started">Download started</string>
    <!--
        Variable %1$s is a file name
        Example:
            "schedule.pdf successfully downloaded"
    -->
    <string name="common_in_app_notification_download_complete">%1$s successfully downloaded</string>
    <!--
        Variable %1$d is a number of files which were successfully downloaded
        Example:
            "1 file successfully downloaded"
            "17 files successfully downloaded"
    -->
    <plurals name="common_in_app_notification_files_download_complete">
        <item quantity="one">%1$d file successfully downloaded</item>
        <item quantity="other">%1$d files successfully downloaded</item>
    </plurals>
    <!--
        Variable %1$s is a file name
        Examples:
            "Downloading schedule.pdf failed"
    -->
    <string name="common_in_app_notification_downloading_failed">Downloading %1$s failed</string>
    <string name="common_in_app_notification_download_failed">Download failed</string>
    <!--
        Variable %1$s is one of "operation_open_document", "operation_create_document", "operation_take_picture"
        Examples:
            "System could not found any application that could open a document"
            "System could not found any application that could take a picture"
    -->
    <string name="common_in_app_notification_activity_not_found">System could not find any application that could %1$s</string>
