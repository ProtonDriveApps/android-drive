<resources>
    <string name="albums_title">Albums</string>
    <string name="albums_new_album_name_hint">Album name</string>
    <string name="albums_new_album_created_successfully">Album created</string>
    <string name="albums_filter_all">@string/common_all</string>
    <string name="albums_filter_my_albums">My albums</string>
    <string name="albums_filter_shared_by_me">@string/shared_by_me_title</string>
    <string name="albums_filter_shared_with_me">@string/shared_with_me_title</string>
    <!--
        Variable %1$s is a number of photos in the album
        Example:
            "1 item"
            "345 items"
    -->
    <plurals name="albums_photo_count">
        <item quantity="one">%1$s item</item>
        <item quantity="other">%1$s items</item>
    </plurals>
    <string name="albums_delete_album_action">Delete album</string>
    <string name="albums_delete_album_dialog_title">Delete album?</string>
    <string name="albums_delete_album_dialog_description">This will only delete the album. Your photos will remain in your timeline. This action cannot be undone.</string>
    <string name="albums_delete_album_dialog_description_with_children">Some photos in this album are not saved to your timeline. Deleting this album will permanently delete those photos.\nWould you like to save them before removing?</string>
    <string name="albums_delete_album_dialog_cancel_action">@string/common_cancel_action</string>
    <string name="albums_delete_album_dialog_delete_album_action">@string/albums_delete_album_action</string>
    <string name="albums_delete_album_dialog_delete_without_saving_action">Delete without saving</string>
    <string name="albums_delete_album_dialog_save_and_delete_action">Save photos and delete</string>
    <string name="albums_add_zero_to_album_button">Add to album</string>
    <!--
        Variable %1$d is a number of photos to add to album
        Example:
            "Add 1 to album"
            "Add 33 to album"
    -->
    <plurals name="albums_add_non_zero_to_album_button">
        <item quantity="one">Add %1$d to album</item>
        <item quantity="other">Add %1$d to album</item>
    </plurals>
    <string name="albums_set_album_as_cover_success">Set as album cover</string>
    <!--
        Variable %1$d is a number of photos added to album
        Example:
            "1 item added to album"
            "17 items added to album"
    -->
    <plurals name="in_app_notification_add_to_album_success">
        <item quantity="one">%1$d item added to album</item>
        <item quantity="other">%1$d items added to album</item>
    </plurals>
    <!--
        Variable %1$d is a number of photos that were not added because they already exist
        Example:
            "1 item already exists"
            "7 items already exist"
    -->
    <plurals name="in_app_notification_add_to_album_already_exists">
        <item quantity="one">%1$d item already exists</item>
        <item quantity="other">%1$d items already exist</item>
    </plurals>
    <!--
        Variable %1$d is a number of photos that were not added because adding failed
        Example:
            "1 item failed"
            "23 items failed"
    -->
    <plurals name="in_app_notification_add_to_album_failed">
        <item quantity="one">%1$d item failed</item>
        <item quantity="other">%1$d items failed</item>
    </plurals>
    <!--
        Variable %1$d is a number of photos removed from album
        Example:
            "1 item removed from album"
            "13 items removed from album"
    -->
    <plurals name="in_app_notification_remove_from_album_success">
        <item quantity="one">%1$d item removed from album</item>
        <item quantity="other">%1$d items removed from album</item>
    </plurals>
    <!--
        Variable %1$d is a number of photos that were not removed because removing failed
        Example:
            "1 item failed"
            "23 items failed"
    -->
    <plurals name="in_app_notification_removed_from_album_failed">
        <item quantity="one">%1$d item failed</item>
        <item quantity="other">%1$d items failed</item>
    </plurals>
    <string name="albums_empty_album_screen_title">No photos</string>
    <string name="albums_empty_album_screen_description">Photos added by you or other members will appear here</string>
    <string name="albums_empty_albums_list_screen_title">No albums</string>
    <string name="albums_empty_albums_list_screen_description">The albums that you create or join will appear here</string>
</resources>
