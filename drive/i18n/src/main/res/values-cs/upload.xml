<resources>
  <!--
        Variable %1$s is the file name if available
        Variable %2$s is message field from exception
        Example:
            "Uploading file My file.jpg failed with reason: Something went wrong"
    -->
  <string name="files_upload_failure_with_description">Nahrávání souboru %1$s selhalo z důvodu: %2$s</string>
  <!--
        Variable %1$d is a quantity of files that are being uploaded
        Variable %2$s is a folder name where files are being uploaded to
        Example:
            "1 file is being uploaded to: Files"
            "3 files are being uploaded to: Pictures"
    -->
  <plurals name="files_upload_being_uploaded_notification">
    <item quantity="one">%1$d soubor je nahráván do: %2$s</item>
    <item quantity="few">%1$d soubory jsou nahrávány do: %2$s</item>
    <item quantity="many">%1$d souborů je nahráváno do: %2$s</item>
    <item quantity="other">%1$d souborů je nahráváno do: %2$s</item>
  </plurals>
  <!--
        Variable %1$d is a quantity of files that failed to be uploaded
        Variable %2$s is a folder name where files failed to be uploaded to
        Example:
            "1 file failed to be uploaded to: Files"
            "3 files failed to be uploaded to: Pictures"
    -->
  <plurals name="files_upload_failed_notification">
    <item quantity="one">%1$d soubor se nepodařilo nahrát do: %2$s</item>
    <item quantity="few">%1$d soubory se nepodařilo nahrát do: %2$s</item>
    <item quantity="many">%1$d souborů se nepodařilo nahrát do: %2$s</item>
    <item quantity="other">%1$d souborů se nepodařilo nahrát do: %2$s</item>
  </plurals>
  <!--
        Variable %1$d is a quantity of files that are being ignored
        Variable %2$s is a folder name where files are being ignored to
        Example:
            "1 file failed to be uploaded to: Files"
            "3 files failed to be uploaded to: Pictures"
    -->
  <plurals name="files_upload_being_ignored_notification">
    <item quantity="one">%1$d soubor byl ignorován</item>
    <item quantity="few">%1$d soubory byly ignorovány</item>
    <item quantity="many">%1$d souborů bylo ignorováno</item>
    <item quantity="other">%1$d souborů bylo ignorováno</item>
  </plurals>
  <string name="files_upload_preparing">Příprava nahrání. Toto může chvíli trvat.</string>
  <!--
        Variable %1$s is the maximum number of uploading files
        Example:
            "Uploads are currently limited to 250 files at a time. Try again with fewer files."
    -->
  <string name="files_upload_limit_reached">Nahrávání je nyní omezeno na %1$d souborů zároveň. Zkuste to znovu s méně soubory.</string>
  <!--
        Variable %1$d is a quantity of items that are being used
        Example:
            "1 item"
            "33 items"
    -->
  <plurals name="number_of_items">
    <item quantity="one">%1$d položka</item>
    <item quantity="few">%1$d položky</item>
    <item quantity="many">%1$d položek</item>
    <item quantity="other">%1$d položek</item>
  </plurals>
  <string name="files_upload_verification_failed">Nahrávání selhalo: Ověření dat selhalo</string>
</resources>
