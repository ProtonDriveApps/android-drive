<resources>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
  <plurals name="trash_operation_successful_format">
    <item quantity="one">1 položka přesunuta do koše</item>
    <item quantity="few">%1$d položky přesunuty do koše</item>
    <item quantity="many">%1$d položek přesunuto do koše</item>
    <item quantity="other">%1$d položek přesunuto do koše</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
  <plurals name="trash_restore_operation_successful_format">
    <item quantity="one">1 položka obnovena</item>
    <item quantity="few">%d položky obnoveny</item>
    <item quantity="many">%d položek obnoveno</item>
    <item quantity="other">%d položek obnoveno</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="one">1 položka trvale smazána</item>
    <item quantity="few">%d položky trvale smazány</item>
    <item quantity="many">%d položek trvale smazáno</item>
    <item quantity="other">%d položek trvale smazáno</item>
  </plurals>
  <string name="trash_empty_operation_successful">Koš vyprázdněn</string>
  <string name="trash_error_occurred_sending_to_trash">Při přesouvání souborů do koše nastala chyba</string>
  <string name="trash_error_occurred_restoring_from_trash">Při pokusu o obnovení souboru (souborů) došlo k chybě</string>
  <string name="trash_error_occurred_deleting_from_trash">Při pokusu o smazání souboru (souborů) došlo k chybě</string>
  <string name="trash_error_occurred_emptying_trash">Při vyprazdňování koše došlo k chybě</string>
</resources>
