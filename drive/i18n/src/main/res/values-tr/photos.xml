<resources>
  <string name="photos_title">Fotoğraflar</string>
  <string name="photos_permissions_title">Fotoğraflarınızı ve videolarınızı şifreleyerek yedekleyin</string>
  <string name="photos_permissions_section_1_title">Anılarınızı koruyun</string>
  <string name="photos_permissions_section_1_description">Fotoğraflarınız tam gizlilik sağlamak için uçtan uca şifrelenir.</string>
  <string name="photos_permissions_section_2_title">Otomatik yedekler</string>
  <string name="photos_permissions_section_2_description">Fotoğraflar Wi-Fi üzerinden özgün kalitesinde düzenli olarak yedeklenir.</string>
