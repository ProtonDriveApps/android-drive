    <item quantity="one">%1$s öge kaldı</item>
    <item quantity="other">%1$s öge kaldı</item>
  </plurals>
  <!--
        Variable %1$s is a quantity (formatted) of photos saved
        Example:
            "1 item saved"
            "12 345 item saved"
