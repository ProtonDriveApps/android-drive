  <string name="photos_permission_rational_img_text">Erişime izin ver</string>
  <!--
        Variable %1$s is an application name
        Example:
            "Proton Drive needs full access to your photos"
    -->
  <string name="photos_permission_rational_title">%1$s için fotoğraflarınıza tam erişim izni vermelisiniz</string>
  <!--
        Variable %1$s is an application name
        Example:
            "...available across all your devices.
             Proton Drive takes your privacy seriously..."
    -->
  <string name="photos_permission_rational_description">Bu izin, tüm fotoğraflarınızın yedeklendiğinden ve tüm aygıtlarınızda kullanılabilir olduğundan emin olmak için gereklidir.\n%1$s gizliliğinizi ciddiye alır ve fotoğraflarınıza yalnızca eşitleme ve yedekleme amacıyla erişir.</string>
  <string name="photos_permission_rational_confirm_action">Ayarlar</string>
  <string name="photos_permission_rational_dismiss_action">Şimdi değil</string>
