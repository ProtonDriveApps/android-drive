  <string name="photos_separator_current_month">Bu ay</string>
  <string name="photos_encrypted_footer">Uçtan uca şifrelenmiş</string>
  <!--
        Variable %1$s is a folder name, %2$s is a quantity of folders found
        Example:
            "Backing up items from 1 folder: Camera"
            "Backing up items from 3 folders: Camera, Raw, Screenshots"
    -->
  <plurals name="photos_message_folders_setup">
    <item quantity="one">%2$d klasöründeki ögeler yedekleniyor: %1$s</item>
    <item quantity="other">%2$d klasöründeki ögeler yedekleniyor: %1$s</item>
  </plurals>
