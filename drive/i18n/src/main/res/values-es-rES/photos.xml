<resources>
  <string name="photos_title">Fotos</string>
  <string name="photos_permissions_title">Cifra y haz copias de seguridad de tus fotos y videos.</string>
  <string name="photos_permissions_section_1_title">Protege tus recuerdos</string>
  <string name="photos_permissions_section_1_description">Tus fotos están cifradas de extremo a extremo asegurando privacidad total.</string>
  <string name="photos_permissions_section_2_title">Copias de seguridad automáticas</string>
  <string name="photos_permissions_section_2_description">Las fotos se copian regularmente mediante Wifi en su calidad original.</string>
  <string name="photos_permissions_action">Activar la copia de seguridad</string>
  <string name="photos_permission_rational_img_text">Permitir el acceso</string>
  <!--
        Variable %1$s is an application name
        Example:
            "Proton Drive needs full access to your photos"
    -->
  <string name="photos_permission_rational_title">%1$s necesita acceso total a tus fotos.</string>
  <!--
        Variable %1$s is an application name
        Example:
            "...available across all your devices.
             Proton Drive takes your privacy seriously..."
    -->
  <string name="photos_permission_rational_description">Esto es necesario para garantizar que todas tus fotos tengan una copia de seguridad y estén disponibles en todos tus dispositivos.\n%1$s toma en serio tu privacidad y solo accederá a tus fotos con el fin de sincronizarlas y realizar copias de seguridad.</string>
  <string name="photos_permission_rational_confirm_action">Ajustes</string>
  <string name="photos_permission_rational_dismiss_action">Ahora no</string>
  <string name="photos_empty_loading_label_encrypted">Cifrado de extremo a extremo</string>
  <string name="photos_empty_loading_label_progress">La copia de seguridad está en curso.\nPuede tomar un tiempo.</string>
  <string name="photos_separator_current_month">Este mes</string>
  <string name="photos_encrypted_footer">Cifrado de extremo a extremo</string>
  <!--
        Variable %1$s is a folder name, %2$s is a quantity of folders found
        Example:
            "Backing up items from 1 folder: Camera"
            "Backing up items from 3 folders: Camera, Raw, Screenshots"
    -->
  <plurals name="photos_message_folders_setup">
    <item quantity="one">Respaldando elementos de %2$d carpeta: %1$s</item>
    <item quantity="other">Respaldando elementos de %2$d carpetas: %1$s</item>
  </plurals>
  <!--
        Variable %1$s is a folder name
