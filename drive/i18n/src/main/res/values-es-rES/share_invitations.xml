<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Compartir %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">1 persona añadida</item>
    <item quantity="other">%1$d personas añadidas</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">No se puede invitar a 1 persona.</item>
    <item quantity="other">No se puede invitar a %1$d personas.</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Compartiendo con 1 persona</item>
    <item quantity="other">Compartiendo con %1$d personas</item>
  </plurals>
  <string name="common_permission_owner">Propietario</string>
  <string name="common_permission_admin">Administrador</string>
  <string name="common_permission_editor">Editor</string>
  <string name="common_permission_viewer">Lector</string>
  <string name="common_permission_unknown">Desconocido</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (pendiente)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (aceptada)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (desconocida)</string>
  <string name="share_via_invitations_add_people_hint">Añadir personas</string>
  <string name="share_via_invitations_copy_invite_link_action">Copiar el enlace de invitación</string>
  <string name="share_via_invitations_remove_access_action">Eliminar el acceso</string>
  <string name="share_via_invitations_stop_sharing_description">Eliminar el enlace y el acceso para todos</string>
  <string name="share_via_invitations_resend_invite_action">Reenviar la invitación</string>
  <string name="share_via_invitations_resend_invite_success">Se ha reenviado la invitación.</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d miembro</item>
    <item quantity="other">%1$d miembros</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">Añadir un mensaje</string>
  <string name="share_via_invitations_send_message_and_name_label">Incluir el mensaje y el nombre del archivo en el correo de invitación</string>
</resources>
