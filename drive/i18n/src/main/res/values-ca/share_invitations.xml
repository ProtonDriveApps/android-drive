<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Comparteix %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">S\'ha afegit 1 persona.</item>
    <item quantity="other">S\'han afegit %1$d persones.</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">No es pot invitar 1 persona.</item>
    <item quantity="other">No es pot invitar %1$d persones.</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">S\'està compartint amb una persona</item>
    <item quantity="other">S\'està compartint amb %1$d persones</item>
  </plurals>
  <string name="common_permission_owner">Propietari</string>
  <string name="common_permission_admin">Administrador</string>
  <string name="common_permission_editor">Editor</string>
  <string name="common_permission_viewer">Lector</string>
  <string name="common_permission_unknown">Desconegut</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (pendent)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (acceptada)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (desconeguda)</string>
  <string name="share_via_invitations_add_people_hint">Afegeix gent</string>
  <string name="share_via_invitations_copy_invite_link_action">Copia l\'enllaç d\'invitació</string>
  <string name="share_via_invitations_remove_access_action">Esborra l\'accés</string>
  <string name="share_via_invitations_stop_sharing_description">Suprimeix l\'enllaç i elimina l\'accés per a tothom</string>
  <string name="share_via_invitations_resend_invite_action">Torna a enviar la invitació</string>
  <string name="share_via_invitations_resend_invite_success">S\'ha reenviat la invitació</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d membre</item>
    <item quantity="other">%1$d membres</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">Afegeix un missatge</string>
  <string name="share_via_invitations_send_message_and_name_label">Inclou el missatge i el nom del fitxer al correu electrònic d\'invitació</string>
</resources>
