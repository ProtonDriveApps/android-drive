<resources>
  <!--
        Variable %1$s is the file name if available
        Variable %2$s is message field from exception
        Example:
            "Uploading file My file.jpg failed with reason: Something went wrong"
    -->
  <string name="files_upload_failure_with_description">No s\'ha pogut carregar el fitxer %1$s amb motiu: %2$s</string>
  <!--
        Variable %1$d is a quantity of files that are being uploaded
        Variable %2$s is a folder name where files are being uploaded to
        Example:
            "1 file is being uploaded to: Files"
            "3 files are being uploaded to: Pictures"
    -->
  <plurals name="files_upload_being_uploaded_notification">
    <item quantity="one">Pujant %1$d fitxer a: %2$s</item>
    <item quantity="other">Pujant %1$d fitxers a: %2$s</item>
  </plurals>
  <string name="files_upload_preparing">S\'està preparant la pujada. Això pot trigar una estona.</string>
  <!--
        Variable %1$s is the maximum number of uploading files
        Example:
            "Uploads are currently limited to 250 files at a time. Try again with fewer files."
    -->
  <string name="files_upload_limit_reached">Actualment, les pujades són limitades a %1$d fitxers alhora. Torneu a provar amb menys fitxers.</string>
  <!--
        Variable %1$d is a quantity of items that are being used
        Example:
            "1 item"
            "33 items"
    -->
  <plurals name="number_of_items">
    <item quantity="one">%1$d element</item>
    <item quantity="other">%1$d elements</item>
  </plurals>
  <string name="files_upload_verification_failed">Error de càrrega: no s\'han pogut verificar les dades.</string>
</resources>
