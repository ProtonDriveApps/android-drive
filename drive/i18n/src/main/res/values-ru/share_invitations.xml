<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Поделиться %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">Добавлен 1 пользователь</item>
    <item quantity="few">%1$d человек добавлено</item>
    <item quantity="many">%1$d человек добавлено</item>
    <item quantity="other">%1$d человек добавлено</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">1 пользователь не может быть приглашен</item>
    <item quantity="few">%1$d пользователь не может быть приглашен</item>
    <item quantity="many">%1$d пользователь не может быть приглашен</item>
    <item quantity="other">%1$d пользователь не может быть приглашен</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Общий доступ с 1 пользователем</item>
    <item quantity="few">Делимся с %1$d людьми</item>
    <item quantity="many">Делимся с %1$d людьми</item>
    <item quantity="other">Делимся с %1$d людьми</item>
  </plurals>
  <string name="common_permission_owner">Владелец</string>
  <string name="common_permission_admin">Администратор</string>
  <string name="common_permission_editor">Редактор</string>
  <string name="common_permission_viewer">Только просмотр</string>
  <string name="common_permission_unknown">Неизвестный тип</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (в ожидании)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (принято)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (неизвестно)</string>
  <string name="share_via_invitations_add_people_hint">Добавить пользователей</string>
  <string name="share_via_invitations_copy_invite_link_action">Скопировать ссылку-приглашение</string>
  <string name="share_via_invitations_remove_access_action">Отменить доступ</string>
  <string name="share_via_invitations_stop_sharing_description">Удалить ссылку и удалить доступ для всех</string>
  <string name="share_via_invitations_resend_invite_action">Отправить повторное приглашение</string>
  <string name="share_via_invitations_resend_invite_success">Приглашение отправлено повторно</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d член</item>
    <item quantity="few">%1$d участников</item>
    <item quantity="many">%1$d членов</item>
    <item quantity="other">%1$d член</item>
  </plurals>
</resources>
