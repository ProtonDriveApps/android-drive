    <item quantity="many">%d элементов восстановлено</item>
    <item quantity="other">%d элементов восстановлено</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="one">1 элемент удалён безвозвратно</item>
