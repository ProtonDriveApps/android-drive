<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Поділитися %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">Додано 1 людину</item>
    <item quantity="few">Додано %1$d людини</item>
    <item quantity="many">Додано %1$d людей</item>
    <item quantity="other">Додано %1$d людей</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">Не можна запросити 1 людину</item>
    <item quantity="few">Не можна запросити %1$d людини</item>
    <item quantity="many">Не можна запросити %1$d людей</item>
    <item quantity="other">Не можна запросити %1$d людей</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Поширено для 1 людини</item>
    <item quantity="few">Поширено для %1$d людей</item>
    <item quantity="many">Поширено для %1$d людей</item>
    <item quantity="other">Поширено для %1$d людей</item>
  </plurals>
  <string name="common_permission_owner">Власник</string>
  <string name="common_permission_admin">Адміністратор</string>
  <string name="common_permission_editor">Редактор</string>
  <string name="common_permission_viewer">Переглядач</string>
  <string name="common_permission_unknown">Невідомо</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (У черзі)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (Прийнята)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (Невідомо)</string>
  <string name="share_via_invitations_add_people_hint">Додати людей</string>
  <string name="share_via_invitations_copy_invite_link_action">Копіювати посилання запрошення</string>
  <string name="share_via_invitations_remove_access_action">Вилучити доступ</string>
  <string name="share_via_invitations_stop_sharing_description">Видалити посилання та доступ для всіх</string>
  <string name="share_via_invitations_resend_invite_action">Повторно надіслати запрошення</string>
  <string name="share_via_invitations_resend_invite_success">Запрошення знову надіслано</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d учасник</item>
    <item quantity="few">%1$d учасники</item>
    <item quantity="many">%1$d учасників</item>
    <item quantity="other">%1$d учасників</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">Додати повідомлення</string>
  <string name="share_via_invitations_send_message_and_name_label">Додайте повідомлення та назву файлу до листа-запрошення</string>
</resources>
