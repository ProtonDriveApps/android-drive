<resources>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
  <plurals name="trash_operation_successful_format">
    <item quantity="one">1 елемент надіслано до смітника</item>
    <item quantity="few">%1$d елементи надіслано до смітника</item>
    <item quantity="many">%1$d елементів надіслано до смітника</item>
    <item quantity="other">%1$d елементів надіслано до смітника</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
  <plurals name="trash_restore_operation_successful_format">
    <item quantity="one">1 елемент відновлено</item>
    <item quantity="few">%d елементи відновлено</item>
    <item quantity="many">%d елементів відновлено</item>
    <item quantity="other">%d елементів відновлено</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="one">1 елемент остаточно видалено</item>
    <item quantity="few">%d елементи остаточно видалено</item>
    <item quantity="many">%d items deleted permanently</item>
    <item quantity="other">%d елементів остаточно видалено</item>
  </plurals>
