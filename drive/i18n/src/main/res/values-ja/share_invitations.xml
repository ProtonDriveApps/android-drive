<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">%1$sを共有</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="other">%1$d 人が追加されました</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="other">%1$d人は招待できません</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="other">%1$d 人と共有しています</item>
  </plurals>
  <string name="common_permission_owner">所有者</string>
  <string name="common_permission_admin">管理者</string>
  <string name="common_permission_editor">編集者</string>
  <string name="common_permission_viewer">閲覧者</string>
  <string name="common_permission_unknown">不明</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (人が未回答)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s(承認済み)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (不明)</string>
  <string name="share_via_invitations_add_people_hint">人を追加する</string>
  <string name="share_via_invitations_copy_invite_link_action">招待リンクをコピーする</string>
  <string name="share_via_invitations_remove_access_action">アクセス権の削除</string>
  <string name="share_via_invitations_stop_sharing_description">リンクを削除して全員のアクセスを削除</string>
  <string name="share_via_invitations_resend_invite_action">招待を再送信する</string>
  <string name="share_via_invitations_resend_invite_success">招待は再送信されました</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="other">%1$d 人のメンバー</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">メッセージを追加</string>
  <string name="share_via_invitations_send_message_and_name_label">招待メールにはメッセージとファイルが含まれています</string>
</resources>
