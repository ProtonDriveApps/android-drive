  <string name="trash_error_occurred_restoring_from_trash">Wystąpił błąd podczas przywracania plików</string>
  <string name="trash_error_occurred_deleting_from_trash">Wystąpił błąd podczas usuwania plików</string>
  <string name="trash_error_occurred_emptying_trash">Wystąpił błąd podczas opróżniania Kosza</string>
</resources>
