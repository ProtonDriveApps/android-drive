<resources>
  <!--
        Variable %1$s is the file name if available
        Variable %2$s is message field from exception
        Example:
            "Uploading file My file.jpg failed with reason: Something went wrong"
    -->
  <string name="files_upload_failure_with_description">Przesyłanie pliku %1$s nie powiodło się. %2$s</string>
  <!--
        Variable %1$d is a quantity of files that are being uploaded
        Variable %2$s is a folder name where files are being uploaded to
        Example:
            "1 file is being uploaded to: Files"
            "3 files are being uploaded to: Pictures"
    -->
  <plurals name="files_upload_being_uploaded_notification">
    <item quantity="one">%1$d plik został przesłany do folderu %2$s</item>
    <item quantity="few">%1$d pliki zostały przesłane do folderu %2$s</item>
    <item quantity="many">%1$d plików zostało przesłanych do folderu %2$s</item>
    <item quantity="other">%1$d plików zostało przesłanych do folderu %2$s</item>
  </plurals>
  <string name="files_upload_preparing">Przygotowywanie przesyłania. To może zająć chwilę.</string>
  <!--
        Variable %1$s is the maximum number of uploading files
        Example:
            "Uploads are currently limited to 250 files at a time. Try again with fewer files."
    -->
  <string name="files_upload_limit_reached">Przesyłanie jest obecnie ograniczone do %1$d plików jednocześnie. Spróbuj ponownie z mniejszą liczbą plików.</string>
  <!--
        Variable %1$d is a quantity of items that are being used
        Example:
            "1 item"
            "33 items"
    -->
  <plurals name="number_of_items">
    <item quantity="one">%1$d element</item>
    <item quantity="few">%1$d elementów</item>
    <item quantity="many">%1$d elementów</item>
    <item quantity="other">%1$d elementów</item>
  </plurals>
  <string name="files_upload_verification_failed">Przesyłanie nie powiodło się: Weryfikacja danych nie powiodła się</string>
</resources>
