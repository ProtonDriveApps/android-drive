<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">與 %1$s 共享</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="other">已新增 %1$d 個人</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="other">無法邀請 %1$d 個人</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="other">與 %1$d 個人分享</item>
  </plurals>
  <string name="common_permission_owner">擁有者</string>
  <string name="common_permission_admin">系統管理員</string>
  <string name="common_permission_editor">編輯者</string>
  <string name="common_permission_viewer">檢視者</string>
  <string name="common_permission_unknown">未知</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s(擱置中)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s(已接受)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s(未知)</string>
  <string name="share_via_invitations_add_people_hint">新增人員</string>
  <string name="share_via_invitations_copy_invite_link_action">複製邀請連結</string>
  <string name="share_via_invitations_remove_access_action">移除存取權限</string>
  <string name="share_via_invitations_stop_sharing_description">刪除連結並移除所有人的存取權</string>
  <string name="share_via_invitations_resend_invite_action">重新發送邀請</string>
  <string name="share_via_invitations_resend_invite_success">已重新寄送邀請</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="other">%1$d 個成員</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">新增訊息</string>
  <string name="share_via_invitations_send_message_and_name_label">在邀請電子郵件中包含訊息與檔案名稱</string>
</resources>
