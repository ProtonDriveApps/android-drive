<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Dela %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">1 person tillagd</item>
    <item quantity="other">%1$d personer tillagda</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">1 person kan inte bjudas in</item>
    <item quantity="other">%1$d personer kan inte bjudas in</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Delar med 1 person</item>
    <item quantity="other">Delar med %1$d personer</item>
  </plurals>
  <string name="common_permission_owner">Ägare</string>
  <string name="common_permission_admin">Admin</string>
  <string name="common_permission_editor">Redaktör</string>
  <string name="common_permission_viewer">Läsare</string>
  <string name="common_permission_unknown">Okänd</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (väntande)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (accepterad)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (okänd)</string>
  <string name="share_via_invitations_add_people_hint">Lägg till person</string>
  <string name="share_via_invitations_copy_invite_link_action">Kopiera inbjudningslänk</string>
  <string name="share_via_invitations_remove_access_action">Ta bort åtkomst</string>
  <string name="share_via_invitations_stop_sharing_description">Ta bort länk och ta bort åtkomst för alla</string>
  <string name="share_via_invitations_resend_invite_action">Skicka inbjudan igen</string>
  <string name="share_via_invitations_resend_invite_success">Inbjudan skickad igen</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d medlem</item>
    <item quantity="other">%1$d medlemmar</item>
  </plurals>
</resources>
