<resources>
  <!--
        Variable %1$s is the file name if available
        Variable %2$s is message field from exception
        Example:
            "Uploading file My file.jpg failed with reason: Something went wrong"
    -->
  <string name="files_upload_failure_with_description">Berkas %1$s gagal diunggah dengan alasan: %2$s</string>
  <!--
        Variable %1$d is a quantity of files that are being uploaded
        Variable %2$s is a folder name where files are being uploaded to
        Example:
            "1 file is being uploaded to: Files"
            "3 files are being uploaded to: Pictures"
    -->
  <plurals name="files_upload_being_uploaded_notification">
    <item quantity="other">%1$d berkas sedang diunggah ke: %2$s</item>
  </plurals>
  <string name="files_upload_preparing">Mempersiapkan unggahan. Proses ini dapat memakan waktu sejenak.</string>
  <!--
        Variable %1$s is the maximum number of uploading files
        Example:
            "Uploads are currently limited to 250 files at a time. Try again with fewer files."
    -->
  <string name="files_upload_limit_reached">Unggahan saat ini terbatas pada %1$d berkas dalam satu waktu. Silakan coba lagi dengan jumlah berkas yang lebih sedikit.</string>
  <!--
        Variable %1$d is a quantity of items that are being used
        Example:
            "1 item"
            "33 items"
    -->
  <plurals name="number_of_items">
    <item quantity="other">%1$d item</item>
  </plurals>
  <string name="files_upload_verification_failed">Pengunggahan gagal: Verifikasi data gagal</string>
</resources>
