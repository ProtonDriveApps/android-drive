<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Bagikan %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="other">%1$d anggota ditambahkan</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="other">%1$d pengguna tidak dapat diundang</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="other">Dibagikan dengan %1$d orang</item>
  </plurals>
  <string name="common_permission_owner">Pemilik</string>
  <string name="common_permission_admin">Admin</string>
  <string name="common_permission_editor">Penyunting</string>
  <string name="common_permission_viewer">Pengamat</string>
  <string name="common_permission_unknown">Tidak diketahui</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (Menunggu jawaban)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (Diterima)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (Tidak diketahui)</string>
  <string name="share_via_invitations_add_people_hint">Tambah pengguna</string>
  <string name="share_via_invitations_copy_invite_link_action">Salin tautan undangan</string>
  <string name="share_via_invitations_remove_access_action">Hapus akses</string>
  <string name="share_via_invitations_stop_sharing_description">Hapus tautan dan anulir akses bagi semua</string>
  <string name="share_via_invitations_resend_invite_action">Kirim ulang undangan</string>
  <string name="share_via_invitations_resend_invite_success">Undangan dikirim ulang</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="other">%1$d anggota</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">Tambahkan pesan</string>
  <string name="share_via_invitations_send_message_and_name_label">Sertakan pesan dan nama berkas dalam email undangan</string>
</resources>
