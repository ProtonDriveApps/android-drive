<resources>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
  <plurals name="trash_operation_successful_format">
    <item quantity="other">%1$d item dipindahkan ke sampah</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
  <plurals name="trash_restore_operation_successful_format">
    <item quantity="other">%d item dipulihkan</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="other">%d item dihapus secara permanen</item>
  </plurals>
  <string name="trash_empty_operation_successful">Sampah dikosongkan</string>
  <string name="trash_error_occurred_sending_to_trash">Terdapat kesalahan pada saat mencoba memindahkan berkas Anda ke sampah</string>
  <string name="trash_error_occurred_restoring_from_trash">Terdapat kesalahan pada saat mencoba memulihkan berkas Anda</string>
  <string name="trash_error_occurred_deleting_from_trash">Terdapat kesalahan pada saat mencoba menghapus berkas Anda</string>
  <string name="trash_error_occurred_emptying_trash">Terdapat kesalahan pada saat mencoba untuk mengosongkan sampah</string>
</resources>
