<resources>
  <!--
        Variable %1$s is the file name if available
        Variable %2$s is message field from exception
        Example:
            "Uploading file My file.jpg failed with reason: Something went wrong"
    -->
  <string name="files_upload_failure_with_description">Не ўдалося запампаваць файл %1$s. Прычына: %2$s</string>
  <!--
        Variable %1$d is a quantity of files that are being uploaded
        Variable %2$s is a folder name where files are being uploaded to
        Example:
            "1 file is being uploaded to: Files"
            "3 files are being uploaded to: Pictures"
    -->
  <plurals name="files_upload_being_uploaded_notification">
    <item quantity="one">%1$d файл запампоўваецца ў: %2$s</item>
    <item quantity="few">%1$d файлы запампоўваюцца ў: %2$s</item>
    <item quantity="many">%1$d файлаў запампоўваюцца ў: %2$s</item>
    <item quantity="other">%1$d файлаў запампоўваюцца ў: %2$s</item>
  </plurals>
  <!--
        Variable %1$d is a quantity of files that failed to be uploaded
        Variable %2$s is a folder name where files failed to be uploaded to
        Example:
            "1 file failed to be uploaded to: Files"
            "3 files failed to be uploaded to: Pictures"
    -->
  <plurals name="files_upload_failed_notification">
    <item quantity="one">Памылка запампоўвання %1$d файла ў: %2$s</item>
    <item quantity="few">Памылка запампоўвання %1$d файлаў у: %2$s</item>
    <item quantity="many">Памылка запампоўвання %1$d файлаў у: %2$s</item>
    <item quantity="other">Памылка запампоўвання %1$d файлаў у: %2$s</item>
  </plurals>
  <!--
        Variable %1$d is a quantity of files that are being ignored
        Variable %2$s is a folder name where files are being ignored to
        Example:
            "1 file failed to be uploaded to: Files"
            "3 files failed to be uploaded to: Pictures"
    -->
  <plurals name="files_upload_being_ignored_notification">
    <item quantity="one">%1$d файл быў праігнараваны</item>
    <item quantity="few">%1$d файлы былі праігнараваны</item>
    <item quantity="many">%1$d файлаў былі праігнараваны</item>
    <item quantity="other">%1$d файлаў былі праігнараваны</item>
  </plurals>
  <string name="files_upload_preparing">Падрыхтоўка да запампоўвання. Гэта можа заняць некаторы час.</string>
  <!--
        Variable %1$s is the maximum number of uploading files
        Example:
            "Uploads are currently limited to 250 files at a time. Try again with fewer files."
    -->
  <string name="files_upload_limit_reached">У дадзены момант максімальная дазволеная колькасць файлаў для адначасовай запампоўкі – %1$d. Паспрабуйце яшчэ раз з меншай колькасцю файлаў.</string>
  <!--
        Variable %1$d is a quantity of items that are being used
        Example:
            "1 item"
            "33 items"
    -->
  <plurals name="number_of_items">
    <item quantity="one">%1$d элемент</item>
    <item quantity="few">%1$d элементы</item>
    <item quantity="many">%1$d элементаў</item>
    <item quantity="other">%1$d элементаў</item>
  </plurals>
  <string name="files_upload_verification_failed">Збой запампоўвання: даныя не прайшлі спраўджанне</string>
</resources>
