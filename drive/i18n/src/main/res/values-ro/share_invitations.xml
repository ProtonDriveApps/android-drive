<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Partajare %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">o persoană adăugată</item>
    <item quantity="few">%1$d persoane adăugate</item>
    <item quantity="other">%1$d de persoane adăugate</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">O persoană nu poate fi invitată</item>
    <item quantity="few">%1$d persoane nu pot fi invitate</item>
    <item quantity="other">%1$d de persoane nu pot fi invitate</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Partajare cu o persoană</item>
    <item quantity="few">Partajare cu %1$d persoane</item>
    <item quantity="other">Partajare cu %1$d de persoane</item>
  </plurals>
  <string name="common_permission_owner">Proprietar</string>
  <string name="common_permission_admin">Administrator</string>
  <string name="common_permission_editor">Editor</string>
  <string name="common_permission_viewer">Utilizator</string>
  <string name="common_permission_unknown">Necunoscut</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (În așteptare)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (Acceptat)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (Necunoscut)</string>
  <string name="share_via_invitations_add_people_hint">Adăugare persoană</string>
  <string name="share_via_invitations_copy_invite_link_action">Copiere legătură web invitație</string>
  <string name="share_via_invitations_remove_access_action">Eliminare acces</string>
  <string name="share_via_invitations_stop_sharing_description">Ștergere legătură web și anulare acces pentru toți</string>
  <string name="share_via_invitations_resend_invite_action">Retrimitere invitație</string>
  <string name="share_via_invitations_resend_invite_success">Invitația a fost retrimisă.</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d membru</item>
    <item quantity="few">%1$d membri</item>
    <item quantity="other">%1$d de membri</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">Add a message</string>
  <string name="share_via_invitations_send_message_and_name_label">Include message and file name in invite email</string>
</resources>
