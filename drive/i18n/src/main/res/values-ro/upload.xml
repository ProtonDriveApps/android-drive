<resources>
  <!--
        Variable %1$s is the file name if available
        Variable %2$s is message field from exception
        Example:
            "Uploading file My file.jpg failed with reason: Something went wrong"
    -->
  <string name="files_upload_failure_with_description">Încărcarea fișierului %1$s a eșuat având ca motiv: %2$s</string>
  <!--
        Variable %1$d is a quantity of files that are being uploaded
        Variable %2$s is a folder name where files are being uploaded to
        Example:
            "1 file is being uploaded to: Files"
            "3 files are being uploaded to: Pictures"
    -->
  <plurals name="files_upload_being_uploaded_notification">
    <item quantity="one">%1$d fișier este încărcat în: %2$s</item>
    <item quantity="few">%1$d fișiere sunt încărcate în: %2$s</item>
    <item quantity="other">%1$d de fișiere sunt încărcate în: %2$s</item>
  </plurals>
  <!--
        Variable %1$d is a quantity of files that failed to be uploaded
        Variable %2$s is a folder name where files failed to be uploaded to
        Example:
            "1 file failed to be uploaded to: Files"
            "3 files failed to be uploaded to: Pictures"
    -->
  <plurals name="files_upload_failed_notification">
    <item quantity="one">Eșuare încărcare fișiere în %2$s: %1$d</item>
    <item quantity="few">Eșuare încărcare fișiere în %2$s: %1$d</item>
    <item quantity="other">Eșuare încărcare fișiere în %2$s: %1$d</item>
  </plurals>
  <!--
        Variable %1$d is a quantity of files that are being ignored
        Variable %2$s is a folder name where files are being ignored to
        Example:
            "1 file failed to be uploaded to: Files"
            "3 files failed to be uploaded to: Pictures"
    -->
  <plurals name="files_upload_being_ignored_notification">
    <item quantity="one">Fișiere ignorate: %1$d</item>
    <item quantity="few">Fișiere ignorate: %1$d</item>
    <item quantity="other">Fișiere ignorate: %1$d</item>
  </plurals>
  <string name="files_upload_preparing">Se pregătește încărcarea. Ar putea dura un timp.</string>
  <!--
        Variable %1$s is the maximum number of uploading files
        Example:
            "Uploads are currently limited to 250 files at a time. Try again with fewer files."
    -->
  <string name="files_upload_limit_reached">Încărcările sunt momentan limitate la %1$d deodată. Încercați cu fișiere mai puține.</string>
  <!--
        Variable %1$d is a quantity of items that are being used
        Example:
            "1 item"
            "33 items"
    -->
  <plurals name="number_of_items">
    <item quantity="one">%1$d articol</item>
    <item quantity="few">%1$d articole</item>
    <item quantity="other">%1$d de articole</item>
  </plurals>
  <string name="files_upload_verification_failed">Încărcare eșuată: verificarea datelor a eșuat.</string>
</resources>
