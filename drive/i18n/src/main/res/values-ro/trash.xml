<resources>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
  <plurals name="trash_operation_successful_format">
    <item quantity="one">Articole trimise în gunoi: 1</item>
    <item quantity="few">Articole trimise în gunoi: %1$d</item>
    <item quantity="other">Articole trimise în gunoi: %1$d</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
  <plurals name="trash_restore_operation_successful_format">
    <item quantity="one">Articole restaurate: 1</item>
    <item quantity="few">Articole restaurate: %d</item>
    <item quantity="other">Articole restaurate: %d</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="one">Articole șterse permanent: 1</item>
    <item quantity="few">Articole șterse permanent: %d</item>
    <item quantity="other">Articole șterse permanent: %d</item>
  </plurals>
  <string name="trash_empty_operation_successful">Gunoiul s-a golit.</string>
  <string name="trash_error_occurred_sending_to_trash">A apărut o eroare în timp ce s-a încercat trimiterea la gunoi.</string>
  <string name="trash_error_occurred_restoring_from_trash">A apărut o eroare în timpul restaurării.</string>
  <string name="trash_error_occurred_deleting_from_trash">A apărut o eroare în timpul ștergerii.</string>
  <string name="trash_error_occurred_emptying_trash">A apărut o eroare în timpul golirii gunoiului.</string>
</resources>
