<resources>
  <!--
        Variable %1$s is the file name if available
        Variable %2$s is message field from exception
        Example:
            "Uploading file My file.jpg failed with reason: Something went wrong"
    -->
  <string name="files_upload_failure_with_description">L\'importation du fichier %1$s n\'a pas abouti pour la raison suivante : %2$s.</string>
  <!--
        Variable %1$d is a quantity of files that are being uploaded
        Variable %2$s is a folder name where files are being uploaded to
        Example:
            "1 file is being uploaded to: Files"
            "3 files are being uploaded to: Pictures"
    -->
  <plurals name="files_upload_being_uploaded_notification">
    <item quantity="one">%1$d fichier est en cours d\'importation dans %2$s.</item>
    <item quantity="other">%1$d fichiers sont en cours d\'importation dans %2$s.</item>
  </plurals>
  <!--
        Variable %1$d is a quantity of files that failed to be uploaded
        Variable %2$s is a folder name where files failed to be uploaded to
        Example:
            "1 file failed to be uploaded to: Files"
            "3 files failed to be uploaded to: Pictures"
    -->
  <plurals name="files_upload_failed_notification">
    <item quantity="one">%1$d fichier n\'a pas pu être importé dans : %2$s</item>
    <item quantity="other">%1$d fichiers n\'ont pas pu être importés dans : %2$s</item>
  </plurals>
  <!--
        Variable %1$d is a quantity of files that are being ignored
        Variable %2$s is a folder name where files are being ignored to
        Example:
            "1 file failed to be uploaded to: Files"
            "3 files failed to be uploaded to: Pictures"
    -->
  <plurals name="files_upload_being_ignored_notification">
    <item quantity="one">%1$d fichier a été ignoré</item>
    <item quantity="other">%1$d fichiers ont été ignorés</item>
  </plurals>
  <string name="files_upload_preparing">L\'importation est en cours de préparation. Cela peut prendre un certain temps.</string>
  <!--
        Variable %1$s is the maximum number of uploading files
        Example:
            "Uploads are currently limited to 250 files at a time. Try again with fewer files."
    -->
  <string name="files_upload_limit_reached">Les importations sont actuellement limitées à %1$d fichiers en même temps. Réessayez avec moins de fichiers.</string>
  <!--
        Variable %1$d is a quantity of items that are being used
        Example:
            "1 item"
            "33 items"
    -->
  <plurals name="number_of_items">
    <item quantity="one">%1$d élément</item>
    <item quantity="other">%1$d éléments</item>
  </plurals>
  <string name="files_upload_verification_failed">L\'importation n\'a pas abouti : les données n\'ont pas pu être vérifiées.</string>
</resources>
