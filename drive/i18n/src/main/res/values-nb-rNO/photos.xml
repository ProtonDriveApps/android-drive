  <string name="photos_backup_in_app_notification_turned_on">Sikkerhetskopiering av bilder er slått på</string>
  <string name="photos_backup_in_app_notification_turned_off">Sikkerhetskopiering av bilder er slått av</string>
  <string name="photos_backup_state_completed">Sikkerhetskopiering fullført</string>
  <string name="photos_backup_state_uncompleted">Sikkerhetskopiering fullført med problemer</string>
  <string name="photos_backup_state_uncompleted_action">Løs</string>
  <string name="photos_backup_state_preparing">Forbereder sikkerhetskopiering</string>
  <string name="photos_backup_state_encrypting">Krypterer…</string>
  <string name="photos_backup_state_uploading">Sikkerhetskopierer…</string>
  <!--
        Variable %1$s is a quantity (formatted) of photos left to upload
        Example:
            "1 item left"
            "12 345 items left"
    -->
  <plurals name="photos_backup_state_uploading_items_left">
    <item quantity="one">%1$s element gjenstår</item>
    <item quantity="other">%1$s elementer gjenstår</item>
  </plurals>
  <!--
        Variable %1$s is a quantity (formatted) of photos saved
        Example:
            "1 item saved"
            "12 345 item saved"
    -->
  <plurals name="photos_backup_state_completed_items_saved">
    <item quantity="one">%1$s element lagret</item>
    <item quantity="other">%1$s elementer lagret</item>
  </plurals>
  <string name="photos_upsell_title">Aldri gå tom for lagringsplass</string>
  <string name="photos_upsell_description">Oppgrader nå og hold alle minnene dine kryptert og trygt.</string>
  <string name="photos_upsell_dismiss_action">Ikke nå</string>
</resources>
