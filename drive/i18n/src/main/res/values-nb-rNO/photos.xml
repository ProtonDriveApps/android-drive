  <string name="photos_error_local_storage_description">For å fortsette sikkerhetskopieringen må du frigjøre lagringsplass på enheten.</string>
  <string name="photos_error_background_restrictions">Sett batteribruk til \'ubegrenset\' for bakgrunnsappaktivitet</string>
  <string name="photos_error_background_restrictions_description">For konsekvent sikkerhetskopiering i bakgrunnen, juster batteriinnstillingene eller åpne appen ofte.</string>
  <string name="photos_error_background_restrictions_action">Innstillinger</string>
  <string name="photos_empty_title">Ingen sikkerhetskopier enda</string>
