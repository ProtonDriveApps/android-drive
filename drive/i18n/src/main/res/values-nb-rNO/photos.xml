<resources>
  <string name="photos_title">Bilder</string>
  <string name="photos_permissions_title">Krypter og sikkerhetskopier dine bilder og videoer</string>
  <string name="photos_permissions_section_1_title">Ta vare på minnene dine</string>
  <string name="photos_permissions_section_1_description">Bildene dine er ende-til-ende-krypterte, noe som sikrer total personvern.</string>
  <string name="photos_permissions_section_2_title">Automatiske sikkerhetskopier</string>
  <string name="photos_permissions_section_2_description">Bilder sikkerhetskopieres regelmessig over Wi-Fi i sin opprinnelige kvalitet.</string>
  <string name="photos_permissions_action">Slå på sikkerhetskopiering</string>
  <string name="photos_permission_rational_img_text">Tillat tilgang</string>
  <!--
        Variable %1$s is an application name
        Example:
            "Proton Drive needs full access to your photos"
    -->
  <string name="photos_permission_rational_title">%1$s behøver full tilgang til dine bilder</string>
  <!--
        Variable %1$s is an application name
        Example:
            "...available across all your devices.
             Proton Drive takes your privacy seriously..."
    -->
  <string name="photos_permission_rational_description">Dette er nødvendig for å sikre at alle bildene dine er sikkerhetskopiert og tilgjengelige på alle enhetene dine.\n%1$s tar personvernet ditt på alvor og får bare tilgang til bildene dine for synkronisering og sikkerhetskopiering.</string>
  <string name="photos_permission_rational_confirm_action">Innstillinger</string>
  <string name="photos_permission_rational_dismiss_action">Ikke nå</string>
  <string name="photos_empty_loading_label_encrypted">Ende-til-ende-kryptert</string>
  <string name="photos_empty_loading_label_progress">Sikkerhetskopiering pågår.\nDet kan ta litt tid</string>
  <string name="photos_separator_current_month">Denne måneden</string>
  <string name="photos_encrypted_footer">Ende-til-ende-kryptert</string>
  <!--
        Variable %1$s is a folder name, %2$s is a quantity of folders found
        Example:
            "Backing up items from 1 folder: Camera"
            "Backing up items from 3 folders: Camera, Raw, Screenshots"
    -->
  <plurals name="photos_message_folders_setup">
    <item quantity="one">Sikkerhetskopierer elementer fra %2$d mappe: %1$s</item>
    <item quantity="other">Sikkerhetskopierer elementer fra %2$d mapper: %1$s</item>
  </plurals>
  <!--
        Variable %1$s is a folder name
        Example:
            "No Camera folder found for backup"
    -->
  <string name="photos_error_no_folders">Ingen %1$s-mappe funnet for sikkerhetskopiering</string>
  <string name="photos_error_backup_disabled">Sikkerhetskopiering deaktivert</string>
  <string name="photos_error_backup_disabled_action">Slå på</string>
  <string name="photos_error_backup_missing_folder">Sikkerhetskopimappen ble ikke funnet</string>
  <string name="photos_error_backup_failed">Sikkerhetskopiering mislyktes</string>
  <string name="photos_error_backup_temporarily_disabled">Sikkerhetskopiering er midlertidig deaktivert.</string>
  <string name="photos_error_missing_permissions">Mangler tillatelser</string>
  <!--
        Variable %1$s is an application name
        Example:
            For continuous and automatic backups, allow Proton Drive access to All Photos in settings.
    -->
  <string name="photos_error_missing_permissions_description">For kontinuerlig og automatisk sikkerhetskopiering, gi %1$s tilgang til \'alle bilder\' i innstillingene.</string>
