<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">分享 %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="other">已添加 %1$d 人</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="other">无法邀请 %1$d 人</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="other">与 %1$d 人共享</item>
  </plurals>
  <string name="common_permission_owner">所有者</string>
  <string name="common_permission_admin">管理员</string>
  <string name="common_permission_editor">编辑者</string>
  <string name="common_permission_viewer">查看者</string>
  <string name="common_permission_unknown">未知</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (待定)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (已接受)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (未知)</string>
  <string name="share_via_invitations_add_people_hint">添加人员</string>
  <string name="share_via_invitations_copy_invite_link_action">复制邀请链接</string>
  <string name="share_via_invitations_remove_access_action">移除访问权</string>
  <string name="share_via_invitations_stop_sharing_description">删除链接并取消所有人的访问权限</string>
  <string name="share_via_invitations_resend_invite_action">重新发送邀请</string>
  <string name="share_via_invitations_resend_invite_success">邀请已重新发送</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="other">%1$d 位成员</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">添加消息</string>
  <string name="share_via_invitations_send_message_and_name_label">在邀请电子邮件中包含消息和文件名</string>
</resources>
