<resources>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
  <plurals name="trash_operation_successful_format">
    <item quantity="one">1 item foi enviado para o lixo.</item>
    <item quantity="other">%1$d itens foram enviados para o lixo.</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
  <plurals name="trash_restore_operation_successful_format">
    <item quantity="one">1 item restaurado</item>
    <item quantity="other">%d items restaurados</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="one">1 item eliminado permanentemente</item>
    <item quantity="other">%d itens eliminados permanentemente</item>
  </plurals>
  <string name="trash_empty_operation_successful">Lixo esvaziado</string>
  <string name="trash_error_occurred_sending_to_trash">Ocorreu um erro ao tentar enviar o(s) seu(s) ficheiro(s) para o lixo</string>
  <string name="trash_error_occurred_restoring_from_trash">Ocorreu um erro ao tentar restaurar um ou mais ficheiros.</string>
  <string name="trash_error_occurred_deleting_from_trash">Ocorreu um erro ao tentar eliminar um ou mais ficheiros.</string>
  <string name="trash_error_occurred_emptying_trash">Ocorreu um erro ao tentar vaziar o lixo.</string>
</resources>
