<resources>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items sent to trash"
    -->
  <plurals name="trash_operation_successful_format">
    <item quantity="one">Predmet poslan v smetnjak</item>
    <item quantity="two">%1$d items sent to trash</item>
    <item quantity="few">%1$d items sent to trash</item>
    <item quantity="other">%1$d items sent to trash</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items restored"
    -->
  <plurals name="trash_restore_operation_successful_format">
    <item quantity="one">1 predmet je bil obnovljen.</item>
    <item quantity="two">%d items restored</item>
    <item quantity="few">%d items restored</item>
    <item quantity="other">%d items restored</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "5 items deleted permanently"
    -->
  <plurals name="trash_delete_operation_successful_format">
    <item quantity="one">1 predmet trajno izbrisan</item>
    <item quantity="two">%d items deleted permanently</item>
    <item quantity="few">%d items deleted permanently</item>
    <item quantity="other">%d items deleted permanently</item>
  </plurals>
  <string name="trash_empty_operation_successful">Smetnjak je prazen</string>
  <string name="trash_error_occurred_sending_to_trash">Pri poskusu pošiljanja vaše datoteke v smetnjak je prišlo do napake</string>
  <string name="trash_error_occurred_restoring_from_trash">Med poskusom obnovitve datotek je prišlo do napake</string>
  <string name="trash_error_occurred_deleting_from_trash">Med poskusom brisanja datotek je prišlo do napake</string>
  <string name="trash_error_occurred_emptying_trash">Med poskusom praznjenja smetnjaka je prišlo do napake</string>
</resources>
