<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Skupna raba %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">Dodan 1 uporabnik</item>
    <item quantity="two">Dodana %1$d uporabnika</item>
    <item quantity="few">Dodani %1$d uporabniki</item>
    <item quantity="other">Dodanih %1$d uporabnikov</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">1 oseba ne more biti povabljena</item>
    <item quantity="two">%1$d osebi ne moreta biti povabljeni</item>
    <item quantity="few">%1$d osebe ne morejo biti povabljene</item>
    <item quantity="other">%1$d oseb ne morejo biti povabljenih</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">V skupni rabi z 1 uporabnikom</item>
    <item quantity="two">V skupni rabi z %1$d uporabnikoma</item>
    <item quantity="few">V skupni rabi s %1$d uporabniki</item>
    <item quantity="other">V skupni rabi s %1$d uporabniki</item>
  </plurals>
  <string name="common_permission_owner">Lastnik</string>
  <string name="common_permission_admin">Skrbnik</string>
  <string name="common_permission_editor">Urednik</string>
  <string name="common_permission_viewer">Pregledovalec</string>
  <string name="common_permission_unknown">Neznano</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (v teku)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (sprejeto)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (neznano)</string>
  <string name="share_via_invitations_add_people_hint">Dodaj osebe</string>
  <string name="share_via_invitations_copy_invite_link_action">Kopiraj povezavo do povabila</string>
  <string name="share_via_invitations_remove_access_action">Odstrani dostop</string>
  <string name="share_via_invitations_stop_sharing_description">Izbriši povezavo in odstrani dostop za vse.</string>
  <string name="share_via_invitations_resend_invite_action">Ponovno pošlji povabilo.</string>
  <string name="share_via_invitations_resend_invite_success">Povabilo je bilo ponovno poslano.</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d član</item>
    <item quantity="two">%1$d člana</item>
    <item quantity="few">%1$d člani</item>
    <item quantity="other">%1$d članov</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">Dodaj sporočilo</string>
  <string name="share_via_invitations_send_message_and_name_label">Include message and file name in invite email</string>
