<resources>
  <!--
        Variable %1$s is the link name
        Example:
            "Share Document"
    -->
  <string name="title_share_via_invitations">Del %1$s</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons added"
    -->
  <plurals name="share_via_invitations_person_added">
    <item quantity="one">1 person tilføjet</item>
    <item quantity="other">%1$d personer tilføjet</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "2 persons cannot be invited"
    -->
  <plurals name="share_via_invitations_person_added_error">
    <item quantity="one">1 person kan ikke inviteres</item>
    <item quantity="other">%1$d personer kan ikke inviteres</item>
  </plurals>
  <!--
        Variable %1$d is a number
        Examples:
            "Sharing with 2 persons"
    -->
  <plurals name="share_via_invitations_sharing_with">
    <item quantity="one">Deles med 1 person</item>
    <item quantity="other">Deles med %1$d personer</item>
  </plurals>
  <string name="common_permission_owner">Ejer</string>
  <string name="common_permission_admin">Admin</string>
  <string name="common_permission_editor">Redaktør</string>
  <string name="common_permission_viewer">Seer</string>
  <string name="common_permission_unknown">Ukendt</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Pending)"
    -->
  <string name="share_via_invitations_external_invitation_pending">%1$s (Afventer)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Accepted)"
    -->
  <string name="share_via_invitations_external_invitation_accepted">%1$s (Godkendt)</string>
  <!--
        Variable %1$s is an email
        Examples:
            "user@mail.com (Unknown)"
    -->
  <string name="share_via_invitations_external_invitation_unknown">%1$s (Ukendt)</string>
  <string name="share_via_invitations_add_people_hint">Tilføj personer</string>
  <string name="share_via_invitations_copy_invite_link_action">Kopiér invitationslink</string>
  <string name="share_via_invitations_remove_access_action">Fjern adgang</string>
  <string name="share_via_invitations_stop_sharing_description">Slet link og fjern adgang for alle</string>
  <string name="share_via_invitations_resend_invite_action">Gensend invitation</string>
  <string name="share_via_invitations_resend_invite_success">Invitationen er gensendt</string>
  <!--
        Variable %1$d is a number
        Examples:
            "2 members"
    -->
  <plurals name="share_via_invitations_group_members">
    <item quantity="one">%1$d medlem</item>
    <item quantity="other">%1$d medlemmer</item>
  </plurals>
  <string name="share_via_invitations_message_placeholder">Add a message</string>
  <string name="share_via_invitations_send_message_and_name_label">Include message and file name in invite email</string>
</resources>
