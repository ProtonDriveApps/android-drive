  <string name="photos_backup_in_app_notification_turned_on">Sikkerhedskopiering af fotos er slået til</string>
  <string name="photos_backup_in_app_notification_turned_off">Sikkerhedskopiering af fotos er slået fra</string>
  <string name="photos_backup_state_completed">Sikkerhedskopiering færdig</string>
  <string name="photos_backup_state_uncompleted">Sikkerhedskopiering gennemført med problemer</string>
  <string name="photos_backup_state_uncompleted_action">Løs</string>
  <string name="photos_backup_state_preparing">Klargøring til sikkerhedskopiering</string>
  <string name="photos_backup_state_encrypting">Krypterer…</string>
  <string name="photos_backup_state_uploading">Sikkerhedskopierer…</string>
