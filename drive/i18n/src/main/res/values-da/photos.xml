<resources>
  <string name="photos_title">Fotos</string>
  <string name="photos_permissions_title">Kryptér og sikkerhedskopiér dine fotos og videoer</string>
  <string name="photos_permissions_section_1_title">Beskyt dine minder</string>
  <string name="photos_permissions_section_1_description">Dine fotos er end-to-end-krypterede, hvilket sikrer fuldstændig fortrolighed.</string>
  <string name="photos_permissions_section_2_title">Automatisk sikkerhedskopiering</string>
  <string name="photos_permissions_section_2_description">Fotos sikkerhedskopieres regelmæssigt over WiFi i deres originale kvalitet.</string>
  <string name="photos_permissions_action">Slå sikkerhedskopier til</string>
  <string name="photos_permission_rational_img_text">Tillad adgang</string>
  <!--
        Variable %1$s is an application name
        Example:
            "Proton Drive needs full access to your photos"
    -->
  <string name="photos_permission_rational_title">%1$s skal have fuld adgang til dine fotos</string>
  <!--
        Variable %1$s is an application name
        Example:
            "...available across all your devices.
             Proton Drive takes your privacy seriously..."
    -->
  <string name="photos_permission_rational_description">Dette er nødvendigt for at sikre, at alle dine fotos er sikkerhedskopieret og tilgængelige på alle dine enheder.\n%1$s tager dit privatliv alvorligt og tilgå dine fotos med henblik på synkronisering og sikkerhedskopiering.</string>
  <string name="photos_permission_rational_confirm_action">Indstillinger</string>
  <string name="photos_permission_rational_dismiss_action">Ikke nu</string>
  <string name="photos_empty_loading_label_encrypted">End-to-end krypteret</string>
  <string name="photos_empty_loading_label_progress">Sikkerhedskopiering er i gang.\nDet kan tage noget tid</string>
  <string name="photos_separator_current_month">Denne måned</string>
  <string name="photos_encrypted_footer">End-to-end krypteret</string>
  <!--
        Variable %1$s is a folder name, %2$s is a quantity of folders found
        Example:
            "Backing up items from 1 folder: Camera"
            "Backing up items from 3 folders: Camera, Raw, Screenshots"
    -->
  <plurals name="photos_message_folders_setup">
    <item quantity="one">Sikkerhedskopierer elementer fra %2$d-mappen: %1$s</item>
    <item quantity="other">Sikkerhedskopierer elementer fra %2$d-mapper: %1$s</item>
  </plurals>
  <!--
        Variable %1$s is a folder name
